{
  "name": "ioc-extractor",
  "version": "2.5.3",
  "description": "IOC(Indicator of Compromise) extractor",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "prepare": "npm run build",
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "coverage": "jest --coverage && cat ./coverage/lcov.info | coveralls",
    "lint": "eslint src --ext .ts",
    "lint:fix": "npm run lint -- --fix",
    "prettier": "prettier src/**/*.ts",
    "prettier:write": "prettier --write src/**/*.ts",
    "docs:build": "typedoc"
  },
  "bin": {
    "ioc-extractor": "dist/bin/ioc-extractor.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "npm run lint:fix",
      "npm run prettier:write"
    ]
  },
  "nyc": {
    "include": [
      "src/*.ts",
      "src/**/*.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "json",
      "html",
      "text"
    ],
    "sourceMap": true
  },
  "keywords": [
    "IOC",
    "Indicator of Compromise"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ninoseki/ioc-extractor.git"
  },
  "author": "Manabu Niseki",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ninoseki/ioc-extractor/issues"
  },
  "homepage": "https://github.com/ninoseki/ioc-extractor",
  "devDependencies": {
    "@types/array-unique": "0.3.0",
    "@types/benchmark": "1.0.33",
    "@types/jest": "26.0.15",
    "@types/node": "14.14.2",
    "@types/punycode": "2.1.0",
    "@types/uuid": "8.3.0",
    "@typescript-eslint/eslint-plugin": "4.5.0",
    "@typescript-eslint/parser": "4.5.0",
    "benny": "3.6.14",
    "coveralls": "3.1.0",
    "eslint": "7.11.0",
    "eslint-config-prettier": "6.14.0",
    "eslint-plugin-jest": "24.1.0",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-simple-import-sort": "5.0.3",
    "husky": "4.3.0",
    "jest": "26.6.0",
    "lint-staged": "10.4.2",
    "prettier": "2.1.2",
    "ts-jest": "26.4.1",
    "ts-loader": "8.0.6",
    "ts-node": "9.0.0",
    "typedoc": "0.19.2",
    "typescript": "4.0.3"
  },
  "dependencies": {
    "array-unique": "^0.3.2",
    "commander": "^6.1.0",
    "get-stdin": "^8.0.0",
    "punycode": "^2.1.1",
    "threads": "^1.6.3",
    "uuid": "^8.3.1"
  }
}
